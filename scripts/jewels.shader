textures/jewels/neon_red
{
	qer_editorimage textures/jewels/neon_red.tga
	q3map_lightimage textures/jewels/neon_blue.tga
	surfaceparm nomarks
	surfaceparm nonsolid
	surfaceparm noimpact
	surfaceparm trans
	surfaceparm nolightmap
	nopicmip

	{
		map textures/jewels/neon_alpha.tga
		rgbGen teamcolor 2
		blendfunc add
	}
}
